{
  "providers": ["python"],
  "buildDir": "ai-user-research",
  "phases": {
    "setup": {
      "nixPkgs": ["nodejs-18_x"]
    }
  },
  "install": {
    "cmds": [
      "cd frontend && npm install && npm run build && cd ..",
      "pip install -r requirements.txt"
    ]
  },
  "start": {
    "cmd": "PYTHONPATH=$PYTHONPATH:/app/ai-user-research cd ai-user-research && python backend/src/main.py"
  }
}
